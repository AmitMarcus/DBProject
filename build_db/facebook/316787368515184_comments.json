        {
            "id": "316787368515184_563267547200497",
            "message": "\u0627\u0641\u062a\u062d \u0627\u0644\u0635\u0648\u0631\u0647",
            "updated_time": "2016-12-17T17:24:47+0000"
        },
        {
            "id": "316787368515184_563412767185975",
            "message": "\u0627\u0644\u0644\u0647\u0645 \u0635\u0644\u064a \u0648\u0633\u0644\u0645 \u0639\u0644\u0649 \u0633\u064a\u062f\u0646\u0627 \u0645\u062d\u0645\u062f \u0627\u0644\u062d\u0628\u064a\u0628 \u0627\u0644\u0645\u0635\u0637\u0641\u0649 \u0635\u0644\u0627\u0629 \u064a\u062a\u0642\u0628\u0644\u0647\u0627 \u0645\u0646\u0627 \u0648\u062a\u0631\u0636\u0649 \u0628\u0647\u0627 \u0639\u0644\u064a\u0646\u0627 \u0648\u062a\u0642\u0631\u0628\u0646\u0627 \u0628\u0647\u0627 \u0627\u0644\u064a\u0643\u2606\u2606\u2606\u2606\u2606\u2606\u2661\u2661\u2661\u2661\u2661\u2661\u2661\u2661\u2661\u2661\u2661\u2661\u00a4\u00a4\u00a4\u00a4\u00a4\u00a4",
            "updated_time": "2016-12-17T17:23:53+0000"
        },
        {
            "id": "316787368515184_563434117183840",
            "message": "\u0631\u0633\u0648\u0644 \u0627\u0644\u0644\u0647 ... \u0634\u0639\u0631 \u0648\u0644\u064a\u062f \u0645\u062d\u062c\u0648\u0628...\n\u0631\u0633\u0648\u0644 \u0627\u0644\u0644\u0647 \u064a\u0627 \u0645\u0646 \u0635\u0644\u0649 \u0639\u0644\u064a\u0647 \u0627\u0644\u0644\u0647...\n\u0631\u0633\u0648\u0644 \u0627\u0644\u0644\u0647 \u064a\u0627 \u0643\u0648\u0643\u0628\u0627 \u0648\u0636\u064a\u0627\u0621...\n\u0631\u0633\u0648\u0644 \u0627\u0644\u0644\u0647 \u0623\u0646\u062a \u0627\u0644\u0647\u0627\u062f\u0649...\n\u0628\u0643\u062a\u0627\u0628 \u0631\u0628\u0643 \u0642\u062f \u0647\u062f\u064a\u062a \u0641\u0624\u0627\u062f\u0649...\n\u0631\u0633\u0648\u0644 \u0627\u0644\u0644\u0647 \u0623\u0646\u062a \u0627\u0644\u0645\u0647\u062a\u062f\u0649...\n\u0628\u0643 \u0641\u0649 \u0643\u0644 \u0645\u0627\u0636\u0649 \u0648\u0623\u062a\u0649...\n\u0631\u0633\u0648\u0644 \u0627\u0644\u0644\u0647 \u064a\u0627 \u062d\u0628\u064a\u0628\u0649...\n\u0628\u062d\u0628\u0643 \u0646\u062d\u064a\u0627 \u0641\u0649 \u0639\u0632\u0629 \u0648\u0625\u0628\u0627\u0626\u0649...\n\u0631\u0633\u0648\u0644 \u0627\u0644\u0644\u0647 \u064a\u0627 \u0645\u0646 \u0642\u0627\u0644 \u0644\u0647 \u0627\u0644\u0644\u0647...\n\u0625\u0642\u0631\u0623 \u0628\u0633\u0645 \u0631\u0628\u0643 \u0627\u0644\u062e\u0627\u0644\u0642 \u0627\u0644\u0648\u0647\u0627\u0628\u0649...\n\u0631\u0633\u0648\u0644 \u0627\u0644\u0644\u0647 \u064a\u0627 \u0645\u0646 \u0639\u0644\u0645\u0643 \u0631\u0628\u0643...\n\u0641\u0635\u0631\u062a \u0628\u0639\u0644\u0645\u0647 \u0623\u0639\u0644\u0645 \u0627\u0644\u0639\u0644\u0645\u0627\u0626\u0649...\n\u0631\u0633\u0648\u0644 \u0627\u0644\u0644\u0647 \u064a\u0627 \u0631\u062d\u0645\u062a\u0627...\n\u0631\u062d\u0645 \u0627\u0644\u0644\u0647 \u0628\u0647\u0627 \u0627\u0644\u062f\u0646\u064a\u0627 \u0645\u0646 \u0627\u0644\u0643\u0641\u0631 \u0648\u0627\u0644\u0625\u0644\u062d\u0627\u062f\u0649..",
            "updated_time": "2016-12-17T17:19:23+0000"
        },
        {
            "id": "316787368515184_563280977199154",
            "message": "\u0627\u0644\u0644\u0647\u0645 \u0635\u0644\u0649 \u0648\u0633\u0644\u0645 \u0639\u0644\u0649 \u0643\u0627\u0645\u0644 \u0627\u0644\u0646\u0648\u0631",
